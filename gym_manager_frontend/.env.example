# Frontend environment variables for Gym Manager (React - CRA)
# Copy to .env.local and fill in values for local development.

# API base URL (optional; used when not in mock mode)
REACT_APP_API_BASE_URL=

# Enable MSW mocks (true/false)
REACT_APP_USE_MOCKS=true

# Supabase configuration (required for auth)
REACT_APP_SUPABASE_URL=https://YOURPROJECTID.supabase.co
REACT_APP_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1...

# Optional: feature flags as JSON
REACT_APP_FEATURE_FLAGS={}
