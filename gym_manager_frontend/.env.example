# Supabase configuration (required)
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_KEY=

# Site URL used for email redirect during signup (optional, defaults to window.location.origin)
REACT_APP_SITE_URL=

# Invite code placeholders (comma-separated). For development/demo; replace with backend validation later.
REACT_APP_INVITE_CODES_TRAINER=DEMO-TRAINER
REACT_APP_INVITE_CODES_OWNER=DEMO-OWNER
# If true, any non-empty code is accepted (for local demos)
REACT_APP_INVITE_CODES_ALLOW_ANY=false
