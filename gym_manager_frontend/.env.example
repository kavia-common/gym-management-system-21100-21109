# Frontend environment variables (example - do not commit real values)
# Copy to .env.local and fill in as needed.

# API base URL (when not using mocks)
REACT_APP_API_BASE_URL=

# Toggle in-app mocks (true/false)
REACT_APP_USE_MOCKS=true

# Supabase credentials (optional for real auth; stub auth works without these)
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_ANON_KEY=
